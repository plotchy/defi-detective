import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/Ownable2StepUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

import "./interfaces/IERC721Staking.sol";
import "./lib/PackedList.sol";
import "./DCCMileage.sol";
import "./RewardBase.sol";
import "./ERC721StakingVault.sol";

/*
* @author Karl
* @notice It's simply ERC721 staking & rewards contract.
* Users receive fixed rewards based on their staked amount.
* see also : RewardBase - This is a fixed reward implementation of RewardBase.
*
* ! This contract is only called by DCCStaking Contract
*/
contract ERC721StakingFixedReward is Initializable, IERC721Staking, RewardBase, AccessControlEnumerableUpgradeable, Ownable2StepUpgradeable, IERC721ReceiverUpgradeable {
    using PackedList for PackedList.Data;

    uint256 private constant PARAM_LIMIT = 20;
    bytes32 public constant STAKE_ROLE = keccak256("STAKE_ROLE");

    IERC721Upgradeable public stakeToken;
    DCCMileage public rewardToken;

    mapping(address => ERC721StakingVault) private vaults;
    mapping(address => PackedList.Data) private tokenStaked;

    /* ====== EVENTS ======= */
    event STAKE(address user, uint256[] tokenIds);
    event UNSTAKE(address user, uint256[] tokenIds);
    event CLAIM(address user);
    event UPDATE_REWARD_PER_SECOND(uint256 newRewardPerSecond);

    /* ====== CONSTRUCTOR ====== */

    function initialize(address _dccStakingContract, IERC721Upgradeable _stakeToken, DCCMileage _rewardToken) external initializer {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(STAKE_ROLE, _dccStakingContract);

        stakeToken = _stakeToken;
        rewardToken = _rewardToken;

        __AccessControlEnumerable_init();
        __Ownable_init();
    }

    /* ====== PUBLIC FUNCTIONS (onlyRole) ====== */

    function stake(address _user, uint256[] memory _tokenIds) external onlyRole(STAKE_ROLE) {
        if (_tokenIds.length == 0) revert NoTokenID();
        if (_tokenIds.length > PARAM_LIMIT) revert ExceedMaxTokenLimit();

        _updateUserAccumulatedRewards(_user);

        if (address(vaults[_user]) == address(0)) {
            vaults[_user] = new ERC721StakingVault(_user);
        }

        address vaultAddress = address(vaults[_user]);
        for (uint i = 0; i < _tokenIds.length; i++) {
            stakeToken.safeTransferFrom(_user, vaultAddress, _tokenIds[i]);
        }

        tokenStaked[_user].addAll(_tokenIds);
        emit STAKE(_user, _tokenIds);
    }

    function unstake(address _user, uint256[] memory _tokenIds, uint256[] memory _tokenIndexes) external onlyRole(STAKE_ROLE) {
        if (_tokenIds.length == 0) revert NoTokenID();
        if (_tokenIds.length > PARAM_LIMIT) revert ExceedMaxTokenLimit();

        if (!tokenStaked[_user].containsAll(_tokenIds, _tokenIndexes)) revert NotStaking();

        _updateUserAccumulatedRewards(_user);

        ERC721StakingVault vault = vaults[_user];
        for (uint i = 0; i < _tokenIds.length; i++) {
            vault.withdrawal(address(stakeToken), _tokenIds[i]);
        }

        require(tokenStaked[_user].removeAll(_tokenIds, _tokenIndexes));
        emit UNSTAKE(_user, _tokenIds);
    }

    function claim(address _user) external onlyRole(STAKE_ROLE) {
        uint256 rewards = _emitRewards(_user);
        if (rewards > 0) {
            rewardToken.mint(_user, rewards);
        }
        emit CLAIM(_user);
    }

    /* ====== VIEW FUNCTIONS ====== */
    function getStakeToken() external view returns (address) {
        return address(stakeToken);
    }

    function countStaking(address _user) external view returns (uint256) {
        return tokenStaked[_user].count;
    }

    function getStaking(address _user) external view returns (uint256[] memory) {
        return tokenStaked[_user].values();
    }

    function isStaking(address _user, uint256[] memory _tokenIds, uint256[] memory _tokenIndexes) external view returns (bool) {
        return tokenStaked[_user].containsAll(_tokenIds, _tokenIndexes);
    }

    function getRewardsToClaim(address _user) external view returns (uint256) {
        return _getRewardsToClaim(_user);
    }

    function getRewardPerSecond() external view returns (uint256) {
        return _getRewardPerSecond();
    }

    /* ====== IERC721Receiver OVERRIDE FUNCTION ====== */

    function onERC721Received(address, address, uint256, bytes calldata) external pure override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    /* ====== REWARD_BASE OVERRIDE FUNCTIONS ====== */

    function _getUserBalance(address _user) internal view override returns (uint256) {
        return tokenStaked[_user].count;
    }

    function _totalRewardShare() internal pure override returns (uint256) {
        return 1;
    }

    /* ====== ADMIN FUNCTIONS ====== */

    function setRewardPerSecond(uint256 _rewardPerSecond) external onlyOwner {
        _setRewardPerSecond(_rewardPerSecond);
        emit UPDATE_REWARD_PER_SECOND(_rewardPerSecond);
    }

}

