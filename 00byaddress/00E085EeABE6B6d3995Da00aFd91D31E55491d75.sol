pragma solidity ^0.8.9;

/*
* @author Karl
* @notice A base contract for calculating rewards in staking.
*
*
* What is the reward index in this contract?
*
* It is the accumulated value of rewards per second.
* For example, if rewardPerSecond is 1, the reward index starts with 0 and becomes 60 after 1 minute.
*
* --------------------------------------------------------------------
* reward
* per      1                          2 (update)
* second
* --------------------------------------------------------------------
* time     1s          60s          100s           300s          600s
* --------------------------------------------------------------------
* index    1           60           100            500           1100
* --------------------------------------------------------------------
* user index                                       500           1100
* (staking point *)                                 *~~~~~~~~~~~~~~*
* --------------------------------------------------------------------
*
* As in the example above, if the user starts staking when the reward index is 500 and
* withdraws when the reward index is 1100. the user's unit reward can be calculated with the difference
* between the reward index. In the example above, the user's reward per staking is 600.
*
*/
contract RewardBase {
    /*
    * rewardPerSecond - The amount of rewards distributed every second.
    * accumulatedRewards - The amount of accumulated rewards that are confirmed and fixed for each user.
    * index - It is the global accumulated value of rewards per second.
    * userIndex - At the last user action, the accumulatedRewards is updated and the reward index is saved to this.
    */
    struct Reward {
        uint256 rewardPerSecond;
        mapping(address => uint256) accumulatedRewards;

        uint256 index;
        uint256 lastIndexedTimestamp;
        mapping(address => uint256) userIndex;
    }
    Reward reward;

    /*
    * @notice To update the reward index. Must be executed when reward per second changes.
    * If it is a fixed reward per staking unit, it can be only called when the rewardPerSecond variable is changed.
    * but, In the case of a ratio rewards staking, each user's staking amount will change the reward of all users,
    * so it should be called for the every actions of user.
    */
    function _updateRewardIndex() internal returns (uint256) {
        if (block.timestamp == reward.lastIndexedTimestamp) {
            return reward.index;
        }

        uint256 newIndex = _getNewIndex();

        if (newIndex != reward.index) {
            reward.index = newIndex;
        }

        reward.lastIndexedTimestamp = block.timestamp;

        return newIndex;
    }

    /**
    * @notice If there is a change in the user's staking,
    * the user rewards so far is calculated and accumulated.
    * Also, it stores the reward index to userIndex.
    */
    function _updateUserAccumulatedRewards(address _user) internal {
        uint256 userIndex = reward.userIndex[_user];
        uint256 newIndex = _updateRewardIndex();

        if (newIndex > userIndex) {
            if (_getUserBalance(_user) > 0) {
                reward.accumulatedRewards[_user] += _getNewRewards(_user);
            }
            reward.userIndex[_user] = newIndex;
        }
    }

    function _getNewIndex() internal view virtual returns (uint256) {
        if (_totalRewardShare() == 0) return reward.index;
        uint256 timeDelta = block.timestamp - reward.lastIndexedTimestamp;
        return reward.rewardPerSecond * timeDelta * 1e18 / _totalRewardShare() + reward.index;
    }

    function _getNewRewards(address _user) internal view virtual returns (uint256) {
        uint256 indexDelta = _getNewIndex() - reward.userIndex[_user];
        return _getUserBalance(_user) * indexDelta / 1e18;
    }

    function _getRewardsToClaim(address _user) internal view virtual returns (uint256) {
        return reward.accumulatedRewards[_user] + _getNewRewards(_user);
    }

    function _getRewardPerSecond() internal view virtual returns (uint256) {
        return reward.rewardPerSecond;
    }

    /*
    * @notice Returns and resets all rewards up to now. It is used to claim.
    */
    function _emitRewards(address _user) internal virtual returns (uint256) {
        _updateUserAccumulatedRewards(_user);
        uint256 amount = reward.accumulatedRewards[_user];
        if (amount > 0) {
            reward.accumulatedRewards[_user] = 0;
        }

        return amount;
    }

    /*
    * @notice If this value is 1, the user will receive a fixed reward regardless of the total amount staked.
    * If this value is the total staking amount, the user will receive a reward equal to their stake ratio.
    */
    function _totalRewardShare() internal view virtual returns (uint256) {}

    function _getUserBalance(address _user) internal view virtual returns (uint256) {}

    function _setRewardPerSecond(uint256 _rewardPerSecond) internal {
        _updateRewardIndex();
        reward.rewardPerSecond = _rewardPerSecond;
    }
}

