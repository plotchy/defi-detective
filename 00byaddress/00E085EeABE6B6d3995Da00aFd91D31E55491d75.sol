/*
* @author Karl
* @notice A data structure created for the special purpose of saving gas when storing items.
* The maximum value of an item is 4095 (2**12 - 1).
*/
library PackedList {
    uint256 constant MASK = 0xfff;
    uint256 constant MAX_INT = 2**256 - 1;
    uint256 constant BITS_PER_ITEM = 12;
    uint256 constant MAX_ITEM_IN_VAR = 21;

    struct Data {
        uint256 count;
        mapping(uint256 => uint256) packedTokenIds;
    }

    function _at(Data storage data, uint256 index) private view returns (uint256) {
        require(index < data.count, "!index");
        uint256 page = index / MAX_ITEM_IN_VAR;
        uint256 idx = index % MAX_ITEM_IN_VAR;
        return MASK & (data.packedTokenIds[page] >> BITS_PER_ITEM * idx);
    }

    function _loadPackedTokenIds(Data storage data) private view returns (uint256[] memory) {
        uint256 maxPage = data.count / MAX_ITEM_IN_VAR;
        uint256[] memory packedTokenIds = new uint256[](maxPage + 1);
        for (uint i = 0; i < maxPage + 1; i++) {
            packedTokenIds[i] = data.packedTokenIds[i];
        }
        return packedTokenIds;
    }

    function _atFrom(uint256[] memory packedTokenIds, uint256 index) private pure returns (uint256) {
        uint256 page = index / MAX_ITEM_IN_VAR;
        uint256 idx = index % MAX_ITEM_IN_VAR;
        return MASK & (packedTokenIds[page] >> BITS_PER_ITEM * idx);
    }

    function _setAt(uint256[] memory packedTokenIds, uint256 index, uint256 tokenId) private pure {
        uint256 page = index / MAX_ITEM_IN_VAR;
        uint256 idx = index % MAX_ITEM_IN_VAR;
        uint256 shift = BITS_PER_ITEM * idx;
        packedTokenIds[page] = (packedTokenIds[page] & ~(MASK << shift)) | tokenId << shift;
    }

    function addAll(Data storage data, uint256[] memory _tokenIds) internal returns (bool) {
        if (_tokenIds.length > MAX_ITEM_IN_VAR) return false;

        uint256 packed0;
        uint256 packed1;
        uint256 count = data.count;
        uint256 page = count / MAX_ITEM_IN_VAR;

        uint256 start0 = count % MAX_ITEM_IN_VAR;
        uint256 end0 = start0 + _tokenIds.length;
        if (end0 > MAX_ITEM_IN_VAR) end0 = MAX_ITEM_IN_VAR;

        uint256 cursor = 0;

        for (uint i = start0; i < end0; i++) {
            packed0 |= _tokenIds[cursor++] << (BITS_PER_ITEM * i);
        }

        uint256 end1 = _tokenIds.length - cursor;

        for (uint i = 0; i < end1; i++) {
            packed1 |= _tokenIds[cursor++] << (BITS_PER_ITEM * i);
        }

        data.count += _tokenIds.length;

        if (packed0 > 0) data.packedTokenIds[page] |= packed0;
        if (packed1 > 0) data.packedTokenIds[page + 1] = packed1;

        return true;
    }

    function containsAll(Data storage data, uint256[] memory _tokenIds, uint256[] memory _indexes) internal view returns (bool) {
        if (_tokenIds.length != _indexes.length) return false;
        for (uint i = 0; i < _tokenIds.length; i++) {
            if (_tokenIds[i] != _at(data, _indexes[i])) {
                return false;
            }
        }
        return true;
    }

    function removeAll(Data storage data, uint256[] memory _tokenIds, uint256[] memory _indexes) internal returns (bool) {
        if (_tokenIds.length == 0 || _tokenIds.length > data.count) return false;
        if (_tokenIds.length != _indexes.length) return false;
        uint256[] memory packedTokenIds = _loadPackedTokenIds(data);

        for (uint i = 0; i < _tokenIds.length; i++) {
            uint256 tokenId = _tokenIds[i];
            uint256 index = _indexes[i];
            uint256 page = index / MAX_ITEM_IN_VAR;
            uint256 idx = index % MAX_ITEM_IN_VAR;
            uint256 shift = BITS_PER_ITEM * idx;

            if (tokenId == MASK & (packedTokenIds[page] >> shift)) {
                packedTokenIds[page] &= ~(MASK << shift);
            } else {
                return false;
            }
        }

        uint sCur = 0;
        uint eCur = data.count - 1;

        while(sCur < eCur) {
            while(_atFrom(packedTokenIds, sCur) != 0 && sCur < data.count) {
                sCur++;
            }
            while(_atFrom(packedTokenIds, eCur) == 0 && eCur > 0) {
                eCur--;
            }

            if (sCur < eCur) {
                _setAt(packedTokenIds, sCur, _atFrom(packedTokenIds, eCur));
                _setAt(packedTokenIds, eCur, 0);
                sCur++;
                eCur--;
            }
        }

        data.count -= _tokenIds.length;
        for (uint i = 0; i < packedTokenIds.length; i++) {
            if (data.packedTokenIds[i] != packedTokenIds[i]) {
                data.packedTokenIds[i] = packedTokenIds[i];
            }
        }

        return true;
    }

    function values(Data storage data) internal view returns (uint256[] memory) {
        uint256[] memory packedTokenIds = _loadPackedTokenIds(data);
        uint256[] memory result = new uint256[](data.count);
        for (uint i = 0; i < data.count; i++) {
            result[i] = _atFrom(packedTokenIds, i);
        }
        return result;
    }
}

